{
    "manifest_version": 3,
    "name": "See The Time",
    "version": "1.0",
    "description": "Displays a clock in new tabs.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "web_accessible_resources": [{
      "resources": ["moment.min.js", "fonts/*.ttf"],
      "matches": ["<all_urls>"]
    }],
    "permissions": [],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}